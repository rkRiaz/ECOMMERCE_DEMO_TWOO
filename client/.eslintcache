ToProps = state => ({\r\n    customer: state.customer,\r\n    busket: state.busket\r\n})\r\n\r\nexport default connect(mapStateToProps, { })(Header)\r\n",["227","228"],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\pages\\Home.js",[],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\index.js",[],["229","230"],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\utils\\setAuthToken.js",[],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\pages\\customer\\CustomerEdit.js",["231"],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\pages\\customer\\CustomerDashboard.js",[],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\pages\\admin\\AdminChangePassword.js",[],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\pages\\admin\\AdminLogin.js",[],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\components\\Loading.js",[],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\store\\reducers\\customerReducer.js",[],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\store\\actions\\types.js",[],["232","233"],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\reportWebVitals.js",[],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\pages\\customer\\ShippingPage.js",["234","235"],"F:\\Web Development\\SOFT_LAB_IT\\Halal Dokan\\application\\client\\src\\pages\\admin\\Customers.js",["236"],"import React, {useState, useEffect} from 'react'\r\nimport \"./Customers.css\"\r\nimport AdminLayout from './AdminLayout'\r\nimport axios from 'axios'\r\nimport Moment from 'react-moment'\r\nimport Loading from '../../components/Loading'\r\nimport {connect} from 'react-redux'\r\n\r\nconst Customers = (props) => {\r\n    const [customers, setCustomers] = useState('')\r\n\r\n    useEffect(() => {  \r\n        axios.get(\